#include<stdio.h>


int main()
{
  printf("Name\t\t:\tSumaiya\n");
  printf("Age\t\t\t:\t18\n");
  printf("Class\t:\tFirst Year\n");
  printf("Div\t\t\t:\tC3\n");
  printf("UIN\t\t\t:\t241p066\n");


  return 0;

}

